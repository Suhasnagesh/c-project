pipeline {
	agent any
	stages {
		stage('build')
		{
			steps {
				sh 'sleep 2; echo "hi suhas"'
			}
		}
	

		stage('deploy')
		{
			steps {
				sh 'sleep 1; echo "hi suhas all ok"'
			}
		}
	stage('suhas')
		{
			steps {
				sh 'sleep 1; echo "hi suhas all ok1"'
			}
		}
		stage('suvi')
		{
			steps {
				sh 'sleep 1; echo "hi suhas all ok2"'
			}
		}
	
	}
}
	


	
